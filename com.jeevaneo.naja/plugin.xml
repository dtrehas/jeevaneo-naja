<plugin>

   <extension point="org.eclipse.emf.ecore.generated_package">
      <package
            uri="http://Naja/1.0"
            class="com.jeevaneo.naja.NajaPackage"
            genModel="model/Naja.genmodel"/>
   </extension>
   <extension
         point="org.eclipse.ui.actionSets">
      <actionSet
            id="org.borgoltz.test.emf.actionSet"
            label="Sample Action Set"
            visible="true">
         <menu
               id="sampleMenu"
               label="Sample &amp;Menu">
            <separator
                  name="sampleGroup">
            </separator>
         </menu>
         <action
               class="org.borgoltz.test.emf.actions.TestAction"
               icon="icons/sample.gif"
               id="org.borgoltz.test.emf.actions.TestAction"
               label="&amp;Sample Action"
               menubarPath="sampleMenu/sampleGroup"
               toolbarPath="sampleGroup"
               tooltip="Hello, Eclipse world">
         </action>
      </actionSet>
   </extension>
   <extension
         point="org.eclipse.ui.popupMenus">
      <objectContribution
            id="com.jeevaneo.naja.contribution1"
            objectClass="com.jeevaneo.naja.Category">
         <menu
               id="com.jeevaneo.naja.menu1"
               label="Naja export"
               path="additions">
            <separator
                  name="group1">
            </separator>
         </menu>
         <action
               class="com.jeevaneo.naja.popup.actions.CsvExportAction"
               enablesFor="*"
               id="com.jeevaneo.naja.newAction"
               label="New Action"
               menubarPath="com.jeevaneo.naja.menu1/group1">
         </action>
      </objectContribution>
   </extension>

</plugin>
